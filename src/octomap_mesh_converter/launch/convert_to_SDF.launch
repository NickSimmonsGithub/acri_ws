<launch>
	<arg name="package"/>
	<arg name="map_path"/>
	<arg name="model_path"/>
	<node pkg="octomap_mesh_converter" type="octomap_to_mesh" name="octomap_dae" output="screen" args="$(arg package) $(arg map_path) $(arg model_path)" required="true">
		<param name="target_resolution" value="0.1"/>
		<param name="script_filename" value="$(find octomap_mesh_converter)/scripts/pointcloud_to_model_0.1.mlx"/>
		<param name="filter_z" value="true"/>
	</node>
</launch>